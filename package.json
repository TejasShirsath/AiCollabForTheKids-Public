{
  "name": "aicollab-for-the-kids",
  "version": "1.0.0",
  "description": "FOR THE KIDS - 50% revenue to charity Children's Hospital",
  "type": "module",
  "scripts": {
    "dev": "cd api && npm run dev",
    "start": "cd api && npm start",
    "migrate": "cd api && npx prisma migrate dev",
    "migrate:deploy": "cd api && npx prisma migrate deploy",
    "studio": "cd api && npx prisma studio",
    "generate": "cd api && npx prisma generate",
    "test": "cd api && npm test",
    "lint": "eslint src --ext .ts,.tsx && prettier --check src",
    "lint:fix": "eslint src --ext .ts,.tsx --fix && prettier --write src",
    "type-check": "tsc --noEmit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "ci": "npm run lint && npm run type-check && npm run test"
  },
  "keywords": [
    "charity",
    "dating",
    "for-the-kids",
    "charity"
  ],
  "author": "Josh Coleman",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ai-Solutions-Store/AiCollabForTheKids.git"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "prettier": "^3.1.0",
    "prisma": "^5.22.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0"
  }
}
